<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="org.uaraven.e" android:installLocation="auto" android:versionName="1.4.0" android:versionCode="15">

    <uses-sdk android:minSdkVersion="8" android:targetSdkVersion="17"/>

    <application android:icon="@drawable/ic_launcher"
                 android:label="@string/app_name"
                 android:theme="@style/ENumbersTheme">
        <activity android:name=".EMainActivity" android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name="EHelpActivity"/>
        <activity android:name="ECodeViewActivity"/>
    </application>

    <supports-screens
            android:largeScreens="true"
            android:normalScreens="true"
            android:smallScreens="true"
            android:anyDensity="true"/>

</manifest> 